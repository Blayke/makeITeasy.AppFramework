{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    },
    {
      "library": "twitter-bootstrap@4.5.0",
      "destination": "wwwroot/lib/twitter-bootstrap/",
      "files": [
        "css/bootstrap.css",
        "js/bootstrap.js"
      ]
    },
    {
      "library": "jquery-validate@1.19.2",
      "destination": "wwwroot/lib/jquery-validate/"
    },
    {
      "library": "jquery-validation-unobtrusive@3.2.11",
      "destination": "wwwroot/lib/jquery-validation-unobtrusive/",
      "files": [
        "jquery.validate.unobtrusive.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "jquery-slidePanel@0.3.5",
      "destination": "wwwroot/lib/jquery-slidePanel/",
      "files": [
        "dist/css/slidePanel.css",
        "dist/jquery-slidePanel.js"
      ]
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.css"
      ]
    },
    {
      "library": "datatables@1.10.21",
      "destination": "wwwroot/lib/datatables/",
      "files": [
        "js/jquery.dataTables.min.js",
        "js/dataTables.bootstrap4.min.js",
        "js/dataTables.dataTables.min.js",
        "css/dataTables.bootstrap4.min.css",
        "css/jquery.dataTables.min.css",
        "images/sort_asc.png",
        "images/sort_asc_disabled.png",
        "images/sort_both.png",
        "images/sort_desc.png",
        "images/sort_desc_disabled.png"
      ]
    }
  ]
}